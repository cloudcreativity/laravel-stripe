{
    "name": "cloudcreativity/laravel-stripe",
    "description": "Laravel integration for Stripe, including Stripe Connect.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Christopher Gammie",
            "email": "info@cloudcreativity.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "illuminate/console": "^5.5|^6.0",
        "illuminate/contracts": "^5.5|^6.0",
        "illuminate/database": "^5.5|^6.0",
        "illuminate/http": "^5.5|^6.0",
        "illuminate/queue": "^5.5|^6.0",
        "illuminate/routing": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0",
        "psr/log": "^1.0",
        "stripe/stripe-php": "^6.40"
    },
    "require-dev": {
        "laravel/cashier": "^8.0|^9.0|^10.0",
        "orchestra/testbench": "^3.5|^4.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "CloudCreativity\\LaravelStripe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CloudCreativity\\LaravelStripe\\Tests\\": "tests/lib"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "CloudCreativity\\LaravelStripe\\ServiceProvider"
            ],
            "aliases": {
                "Stripe": "CloudCreativity\\LaravelStripe\\Facades\\Stripe"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
